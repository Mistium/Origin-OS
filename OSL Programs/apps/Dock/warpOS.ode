window "dimensions" screensize_x 60
window "front"
dango = "data:image/svg+xml;base64,"
closed_apps = "data:image/svg+xml;base64,"
frame "clear"
permission "request" "File Admin"
mainloop:
include "Origin/(C) Users/" ++ username ++ "/dock"
window "y" screensize_y / -2 + 30
if "space".pressed (
  window "stop"
)
square window_width 60 10 : c#000
loc 2 999 30 0
image dango 40
change_x 40
image closed_apps 40
change_x 30
icon "c #666 line 0 15 0 -15" 1
change_x 35

count = 0
loop dock.len (
count += 1
file "open" dock.[count]
s = 1.5
square 40 40 0 0 1
if mouse_touching "s += 0.3"
file "render" s "interactable"
change_x 50
)
c #fff
loc -2 999 -140 10
text "<hour>:<minute>" 10
loc -2 999 -140 -10
text "<day>:<day_number>" 10

loc -2 999 -220
b = battery_percent
bat = "battery_0"
if b > 25 (
bat = "battery_1"
if b > 50 (
bat = "battery_2"
if b > 75 (
bat = "battery_3"
if b > 99 (
bat = "battery_4"
)
)
)
)
if battery_charging (
bat = "battery_charging"
)
icon bat 1.2
text "<b>%" 10 : chx#20
