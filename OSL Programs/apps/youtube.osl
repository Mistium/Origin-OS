
def "load_video" "vid_id"
  network "getsync" "https://yt2html5.com/?id=" ++ vid_id
  data = data."data"
  video_url = data."formats".[1]."url"
  log "sys" data."formats".[1]."url"
  video_title = data."videoDetails"."title"
endef

load_video "dQw4w9WgXcQ"

mainloop:
goto 0 0
